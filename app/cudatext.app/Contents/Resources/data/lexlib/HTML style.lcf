      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'URL'
      StyleName = 'URL'
      TokenType = 9
      Expression = '(?i-)(?<=\burl\()[^\)]+'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'Color'
      StyleName = 'Color'
      TokenType = 6
      Expression = '\#\h{3,6}\b'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'Prop'
      StyleName = 'Prop'
      TokenType = 2
      Expression = '[a-z_\-][\w\-]*(?=\x20*:)'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'Value unit'
      StyleName = 'Value unit'
      TokenType = 7
      Expression = '(?<=\b\d+)[a-z%]+'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
