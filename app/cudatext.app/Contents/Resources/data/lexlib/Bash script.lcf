    '!!, !-1, !50, !w'
    '(( 1%2 )) (( !2 ))  [[ $str -~ ^$'#39'ab'#39' ]]'
    ''
    'function echo {'
    #9'git -a --test "path"'
    '}'
    ''
    'while read line  #comment'
    'do'
    '  test $(test "$line"); '
    '  test "test\"test" '
    '  len=${#line} $(text here)'
    '  if [[ "$len" -lt "$MINLEN" && "$line" =~ \[*\.\] ]]'
    '    then echo'
    '  fi'
    'done'
    ''
    'cat <<-  '#39'indented_without_expansions'#39
    '    ${foo}'
    '    indented_without_expansions'
    ''
    'variable=$(cat <<SETVAR'
