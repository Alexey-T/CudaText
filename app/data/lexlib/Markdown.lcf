      ColumnTo = 0
    end
    item
      DisplayName = 'Link usual'
      StyleName = 'Image'
      StatesAbsent = 3
      TokenType = 11
      Expression = '(?s)(?<=\])\( (\([^\)]*\)|.)*?\) (\{.*?\})?'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'Break line'
      StyleName = 'Comment'
      StatesAbsent = 3
      TokenType = 2
      Expression = '^(([\-\*_])\x20+)+\2?$'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'List'
      StyleName = 'List'
      StatesAbsent = 3
      TokenType = 13
      Expression = 
        '^[\x20\x09]*( \d{1,3}\. '#13#10'| [\-\+\*]'#13#10'| \(?\w?[\.\)]'#13#10') \x20(\[[' +
        '\x20xX]\]\x20)? #GitHub: [ ] [x] [X]'#13#10'| ^\(@[^\)]*\)#pandoc list'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'Codeblock indented'
      StyleName = 'Codeblock'
      StatesAbsent = 3
      TokenType = 4
      Expression = '^(\x20{4,}|\x09{1,}) [^`<>\n\r].*$'
      ColumnFrom = 0
      ColumnTo = 0
    end
    item
      DisplayName = 'Footnote'
      StyleName = 'Footnote'
      StatesAbsent = 3
      TokenType = 7
      Expression = '^\[\^[^\]\^]+\]:.*?($|\z) |'#13#10'\[\^[^\]]+\]'
