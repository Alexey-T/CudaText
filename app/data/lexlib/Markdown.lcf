      ColumnTo = 0
    end
    item
      DisplayName = 'Link desc text'
      StyleName = 'Link'
      StatesAbsent = 3
      TokenType = 11
      Expression = '!?\[ (\[[^\]]*\]|`.+?`|.)*? \]'#13#10'#allow nested [text] and `text`'
